{
    "name": "j-sandaruwan/laravel-job-monitor",
    "description": "Laravel package for tracking queue jobs with status, progress, and detailed execution history",
    "keywords": [
        "laravel",
        "queue",
        "jobs",
        "monitoring",
        "tracking",
        "progress",
        "job-monitor"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "J Sandaruwan",
            "email": "janithsandaruwan29@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "JSandaruwan\\LaravelJobMonitor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JSandaruwan\\LaravelJobMonitor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JSandaruwan\\LaravelJobMonitor\\JobMonitorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
